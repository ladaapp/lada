msgstr "使用 GPU 编码器编码视频："

#: lada/cli/main.py:29
#, python-format
msgid "%(prog)s --input input.mp4 --codec hevc_nvenc --crf 20"
msgstr "%(prog)s --input input.mp4 --codec hevc_nvenc --crf 20"

