msgstr ""
"處理中影片: {done_percent}%|{bar}|已處理: {time_done} ({frames_done}f)"
"{bar_suffix}"

msgid " | Remaining: ? | Speed: ?"
msgstr " | 剩餘的: ? | 速度: ?"

